{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ],
  "functions": {
    "client/src/**/*.{js,ts}": {
      "excludeFiles": "**/*.test.{js,ts}"
    }
  },
  "outputDirectory": "client/dist"
}
